      "settings": {
        "image": "shopify:\/\/shop_images\/imgage_text.png",
        "image_width": 485,
        "box_text_width": 455,
        "layout": "image_first",
        "desktop_content_position": "top",
        "desktop_content_alignment": "left",
        "mobile_content_alignment": "left",
        "padding_top": 90,
        "padding_bottom": 140,
        "padding_top_mb": 60,
        "padding_bottom_mb": 64,
        "show_bottom": true,
        "column_1": "<p>Atomic Fire Balls<br\/>BB Bats<br\/>Beechies Gum <br\/>Bit-O-Honey <br\/>Bonomo's Turkish Taffy<br\/>Bubble Yum <br\/>Candy Buttons on paper tape <br\/>Candy Cigarettes <br\/>Candy Necklace <br\/>Charleston Chews <br\/>Chick-o-Stick <br\/>Chuckles <br\/>Clark Bar Jr <\/p>",
        "column_2": "<p>Dots<br\/>Dubble Bubble Gum<br\/>Gobstoppers<br\/>Jawbreakers<br\/>Kits Taffy<br\/>Laffy Taffy<br\/>Lemonheads<br\/>Lik-M-Aid Fun Dip<br\/>Necco Wafers<br\/>Now & Laters<br\/>Oh Henry<br\/>Peanut Butter Bars<br\/>Pop Rocks<\/p>",
        "column_3": "<p>Root Beer Barrels<br\/>Saf-T-Pops<br\/>Smarties<br\/>Sour Patch Kids<br\/>Spearmint Gum<br\/>Stick Candy<br\/>Sugar Daddy<br\/>Swedish Fish<br\/>Tootsie Pops<br\/>Tootsie RollWarheads<br\/>Wax LipsZotz<\/p>"
      }
